package org.dariochat.app;

public class Sample
{
	double time;
	String window;
	String event;
	String extra;
	
	Sample (double time, String window, String event, String extra)
	{
		this.time = time;
		this.window = window;
		this.event = event;
		this.extra = extra;
	}
	
	public String toString ()
	{
		return Utilities.df3.format(time) + "\t" + window + "\t" + event + "\t\"" + extra + "\"";
	}
}
